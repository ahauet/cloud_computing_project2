{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Zip all files given by a directory",
  "Resources": {
    "Bucket1": {
      "Type": "AWS::S3::Bucket"
    },
    "Zipper": {
      "Properties": {
        "CodeUri": "s3://lingi2145-resources/272d296c608532a3f5f44f709b8d76db",
        "Description": "Zip all files given by a directory",
        "Events": {
          "BucketEvent1": {
            "Properties": {
              "Bucket": {
                "Ref": "Bucket1"
              },
              "Events": [
                "s3:ObjectCreated:*"
              ],
              "Filter": {
                "S3Key": {
                  "Rules": [
                    {
                      "Name": "prefix",
                      "Value": "processed/"
                    },
                    {
                      "Name": "suffix",
                      "Value": ".jpeg"
                    }
                  ]
                }
              }
            },
            "Type": "S3"
          },
          "BucketEvent2": {
            "Properties": {
              "Bucket": {
                "Ref": "Bucket1"
              },
              "Events": [
                "s3:ObjectCreated:*"
              ],
              "Filter": {
                "S3Key": {
                  "Rules": [
                    {
                      "Name": "prefix",
                      "Value": "processed/"
                    },
                    {
                      "Name": "suffix",
                      "Value": ".JPG"
                    }
                  ]
                }
              }
            },
            "Type": "S3"
          },
          "BucketEvent3": {
            "Properties": {
              "Bucket": {
                "Ref": "Bucket1"
              },
              "Events": [
                "s3:ObjectCreated:*"
              ],
              "Filter": {
                "S3Key": {
                  "Rules": [
                    {
                      "Name": "prefix",
                      "Value": "processed/"
                    },
                    {
                      "Name": "suffix",
                      "Value": ".png"
                    }
                  ]
                }
              }
            },
            "Type": "S3"
          },
          "BucketEvent4": {
            "Properties": {
              "Bucket": {
                "Ref": "Bucket1"
              },
              "Events": [
                "s3:ObjectCreated:*"
              ],
              "Filter": {
                "S3Key": {
                  "Rules": [
                    {
                      "Name": "prefix",
                      "Value": "processed/"
                    },
                    {
                      "Name": "suffix",
                      "Value": ".jpg"
                    }
                  ]
                }
              }
            },
            "Type": "S3"
          }
        },
        "Handler": "index.handler",
        "MemorySize": 128,
        "Role": "arn:aws:iam::828843326560:role/Lambda-role",
        "Runtime": "nodejs4.3",
        "Timeout": 54
      },
      "Type": "AWS::Serverless::Function"
    }
  },
  "Transform": "AWS::Serverless-2016-10-31"
}
